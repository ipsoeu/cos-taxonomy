  <rdf:Description rdf:about="https://ipsoeu.github.io/cos-taxonomy/business-event/2019/BE0101">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier>BE0101</dct:identifier>
    <skos:notation>BE0101</skos:notation>
    <skos:prefLabel xml:lang="en">Registering a company</skos:prefLabel>
    <skos:definition xml:lang="en">This business event groups public services that are related to different kinds of activities that have to be done before the business is eligible to operate, for instance registering in the business register, registering as aVATpayer, notifying the residence of your company...</skos:definition>
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/cos-taxonomy/business-event/2019"/>
    <skos:broader rdf:resource="https://ipsoeu.github.io/cos-taxonomy/business-event/2019/BE01"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://ipsoeu.github.io/cos-taxonomy/business-event/2019/BE0102">
