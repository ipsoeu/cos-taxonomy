    <skos:notation>BE0102</skos:notation>
    <skos:prefLabel xml:lang="en">Needing a licence, permit or certificate to start or continue an activity</skos:prefLabel>
    <skos:definition xml:lang="en">This business events groups public services related to the request of a licence, permit, certificate or other official document that have to be acquired before starting to execute a certain activity.</skos:definition>
    <skos:inScheme rdf:resource="https://ipsoeu.github.io/cos-taxonomy/business-event/2019"/>
    <skos:broader rdf:resource="https://ipsoeu.github.io/cos-taxonomy/business-event/2019/BE01"/>
  </rdf:Description>
